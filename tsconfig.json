{
  "compilerOptions": {
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "lib": ["es2020", "DOM"],
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./deploy/dist",
    "noUnusedLocals": false,
    "sourceMap": false,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    // "tsBuildInfoFile": "./deploy/cache/tsc.json",
    "skipLibCheck": true,
    "strict": true,
    "esModuleInterop": true,
  },
  "include": ["./src"]

}
